<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="work.controller.Rule"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <Label layoutX="36.0" layoutY="269.0" prefHeight="32.0" prefWidth="285.0" text="1.每一位评委在第一空中填下三项的成绩">
            <font>
                <Font name="System Bold" size="15.0" />
            </font>
        </Label>
        <Label layoutX="36.0" layoutY="311.0" prefHeight="32.0" prefWidth="442.0" text="2.待五位评委都填完第一空后，按下打分，生成当前选手的总成绩">
            <font>
                <Font name="System Bold" size="15.0" />
            </font>
        </Label>
        <Label layoutX="253.0" layoutY="24.0" text="比赛细则">
            <font>
                <Font name="System Bold Italic" size="14.0" />
            </font>
        </Label>
        <Label layoutX="67.0" layoutY="60.0" prefHeight="19.0" prefWidth="81.0" text="一：比赛流程">
            <font>
                <Font name="System Bold Italic" size="13.0" />
            </font>
        </Label>
        <Label layoutX="36.0" layoutY="97.0" prefHeight="32.0" prefWidth="442.0" text="1.依次点击每一位选手，听取其唱的歌曲">
            <font>
                <Font name="System Bold" size="15.0" />
            </font>
        </Label>
        <Label layoutX="36.0" layoutY="146.0" prefHeight="32.0" prefWidth="285.0" text="2.打完分后，点击下一轮">
            <font>
                <Font name="System Bold Italic" size="15.0" />
            </font>
        </Label>
        <Label layoutX="67.0" layoutY="235.0" prefHeight="19.0" prefWidth="81.0" text="二：打分细则">
            <font>
                <Font name="System Bold Italic" size="13.0" />
            </font>
        </Label>
        <Button layoutX="467.0" layoutY="58.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="56.0" text="返回1" onAction="#return1"/>
        <Button layoutX="467.0" layoutY="101.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="56.0" text="返回2" onAction="#return2"/>
        <Button layoutX="467.0" layoutY="146.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="56.0" text="返回3" onAction="#return3" />
    </children>
</AnchorPane>
